{
    "name": "@braineet/ui",
    "version": "5.2.0",
    "description": "Braineet's UI library",
    "main": "dist/index.js",
    "module": "dist/es/index.js",
    "sideEffects": false,
    "scripts": {
        "storybook": "start-storybook -s ./static -p 3002 --ci",
        "build-storybook": "build-storybook -o ./build",
        "prebuild": "rimraf dist",
        "build:copy-types": "node ./scripts/copy-files.js",
        "build": "yarn build:cjs && yarn build:es",
        "watch": "concurrently \"yarn build:cjs:w\" \"yarn build:es:w\"",
        "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --root-mode upward src -d dist --ignore \"**/*.story.js, **/__tests__\"",
        "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --root-mode upward src -d dist/es --ignore \"**/*.story.js, **/__tests__\"",
        "build:cjs:w": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --root-mode upward src -w --verbose -d dist --ignore \"**/*.story.js, **/__tests__\"",
        "build:es:w": "cross-env NODE_ENV=production BABEL_ENV=es babel --root-mode upward src -w --verbose -d dist/es --ignore \"**/*.story.js, **/__tests__\"",
        "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --root-mode upward src -d dist/esm --ignore \"**/*.story.js, **/__tests__\"",
        "version": "yarn build"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.4",
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@babel/runtime": "^7.10.2",
        "babel-loader": "^8.1.0",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-styled-components": "^1.10.6",
        "concurrently": "^5.1.0",
        "cross-env": "^6.0.3",
        "jest-styled-components": "^7.0.0-beta.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.11.0",
        "styled-components": "^5.0.0-rc.0"
    },
    "dependencies": {
        "body-scroll-lock": "3.0.3",
        "immutable": "^4.0.0-rc.12",
        "is-hotkey": "^0.1.6",
        "is-url": "^1.2.4",
        "react-dates": "^21.3.1",
        "react-dropzone": "^11.0.2",
        "react-focus-lock": "^2.2.1",
        "react-focus-on": "^3.4.1",
        "react-palette": "^1.0.2",
        "react-range": "^1.8.3",
        "react-rnd": "^10.1.1",
        "react-spring": "8.0.4",
        "react-svg": "^11.1.1",
        "react-textarea-autosize": "^7.1.2",
        "react-toastify": "^6.2.0",
        "styled-system": "^5.1.2",
        "tinymce": "^5.1.6"
    },
    "peerDependencies": {
        "react": "^16.11.0",
        "react-dom": "^16.11.0",
        "styled-components": "^5.0.0-rc.0"
    }
}
